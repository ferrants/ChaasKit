{
  "name": "chaaskit-demo",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "react-router dev",
    "build": "react-router build",
    "start": "NODE_ENV=production node server.js",
    "typecheck": "react-router typegen && tsc --noEmit",
    "db:generate": "prisma generate --schema=./prisma/schema",
    "db:push": "prisma db push --schema=./prisma/schema",
    "db:migrate": "prisma migrate dev --schema=./prisma/schema",
    "db:studio": "prisma studio --schema=./prisma/schema",
    "db:sync": "chaaskit-server db:sync"
  },
  "dependencies": {
    "@chaaskit/client": "workspace:*",
    "@chaaskit/db": "workspace:*",
    "@chaaskit/server": "workspace:*",
    "@chaaskit/shared": "workspace:*",
    "@prisma/client": "^6.0.0",
    "@react-router/express": "^7.1.0",
    "@react-router/node": "^7.1.0",
    "concurrently": "^9.2.1",
    "express": "^4.21.0",
    "isbot": "^5.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^7.1.0",
    "react-router-dom": "^7.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@react-router/dev": "^7.1.0",
    "@react-router/fs-routes": "^7.1.0",
    "@types/node": "^22.0.0",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.35",
    "prisma": "^6.0.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.4.0",
    "vite-tsconfig-paths": "^5.0.0"
  }
}
